{
	"name": "sveltekit",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:force": "vite dev --force",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prisma:push": "npx prisma db push",
		"prisma:init": "npx prisma migrate dev --name init",
		"update:svelte": "npm i svelte@latest svelte-check@latest svelte-eslint-parser@latest svelte-preprocess@latest -D",
		"update:sveltekit": "npm i @sveltejs/kit@latest @sveltejs/adapter-auto@latest @sveltejs/adapter-node@latest @sveltejs/vite-plugin-svelte@latest -D",
		"update:authjs": "npm i @auth/core@latest @auth/prisma-adapter@latest @auth/sveltekit@latest -S",
		"update:prisma": "npm i prisma@latest -D && npm i @prisma/client@latest -S",
		"update:eslint": "npm i eslint@latest @typescript-eslint/eslint-plugin@latest @typescript-eslint/parser@latest eslint-config-prettier@latest eslint-plugin-svelte@latest -D",
		"update:tailwind": "npm i tailwindcss@latest @tailwindcss/forms@latest postcss@latest autoprefixer@latest -D",
		"update:vite": "npm i vite@latest vitest@latest -D",
		"update:langchain": "npm i @langchain/core@latest @langchain/openai@latest langchain@latest -S"
	},
	"devDependencies": {
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"@types/eslint": "^9.6.0",
		"@vite-pwa/sveltekit": "^0.6.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"kolorist": "^1.8.0",
		"postcss": "^8.4.40",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.6",
		"prisma": "^5.17.0",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"svelte-eslint-parser": "^0.41.0",
		"svelte-preprocess": "^6.0.2",
		"tailwindcss": "^3.4.7",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.0.0-alpha.54",
		"vite": "^5.3.5",
		"vitest": "^2.0.4",
		"workbox-precaching": "^7.1.0",
		"workbox-routing": "^7.1.0",
		"workbox-strategies": "^7.1.0"
	},
	"dependencies": {
		"@auth/core": "^0.34.1",
		"@auth/prisma-adapter": "^2.4.1",
		"@auth/sveltekit": "^1.4.1",
		"@langchain/core": "^0.2.18",
		"@langchain/openai": "^0.2.5",
		"@prisma/client": "^5.17.0",
		"langchain": "^0.2.11",
		"zod": "^3.22.4",
		"zod-form-data": "^2.0.2"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	}
}
